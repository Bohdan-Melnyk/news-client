<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="361.0" prefWidth="392.0" spacing="10" stylesheets="@styles.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22" fx:controller="com.example.newsclient.HelloController">
    <ComboBox fx:id="timeFilter" promptText="Choose">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Morning" />
                <String fx:value="Day" />
                <String fx:value="Evening" />
            </FXCollections>
        </items>
    </ComboBox>
    <Label fx:id="headlineLabel" styleClass="label" />
   <Pane prefHeight="72.0" prefWidth="352.0" />
    <Label fx:id="descriptionLabel" styleClass="label" />
    <Button fx:id="previousButton" styleClass="button" text="Previous" textAlignment="CENTER" />
    <Button fx:id="nextButton" prefHeight="42.0" prefWidth="94.0" styleClass="button" text="Next" />
</VBox>
